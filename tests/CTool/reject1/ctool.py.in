from Synopsis.getoptions import get_options
from Synopsis.Parsers import C
import sys

args = sys.argv[1:]
output = ''
debug = False
symbols = False
for name, value in get_options(args):
   if name == 'output': output = value
   if name == 'debug': debug = value
   if name == 'symbols': symbols = value

parser = C.Parser()

parser.dump(output = output,
            debug = debug,
            symbols = symbols,
            input = args)
